<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ResizableView">
        <attr name="resize_ratio" format="float"></attr>
        <attr name="resize_base">
            <flag name="width" value="0"></flag>
            <flag name="height" value="1"></flag>
        </attr>
    </declare-styleable>
    <declare-styleable name="NetView">
        <attr name="line_width" format="dimension"></attr>

    </declare-styleable>
    <declare-styleable name="GradientTextView">
        <attr name="color1" format="color" />
        <attr name="color2" format="color" />
        <attr name="color3" format="color" />
    </declare-styleable>
</resources>